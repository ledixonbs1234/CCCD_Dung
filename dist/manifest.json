{
    "version": "1.1.1",
    "name": "CCCD App",
    "manifest_version": 3,
    "action": {
        "default_title": "Click Me",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "16.png",
            "24": "32.jpg",
            "64": "64.png",
            "128": "128.png"
        }
    },
    "options_page": "option.html",
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newTab": "newTab.html"
    },
    "host_permissions": [
        "*://*/*",
        "*://*.example.org/*"
    ],
    "permissions": [
        "tabs",
        "bookmarks",
        "unlimitedStorage",
        "notifications",
        "storage",
        "cookies"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
       
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}